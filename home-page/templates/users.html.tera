<h1>Users</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Hashed password</th>
    <th>Role</th>
    <th>Created</th>
  </tr>

  {% for user in users %}
  <tr>
    <td>{{ user.name }}</td>
    <td>{{ user.hashed_password }}</td>
    <td>{{ user.role }}</td>
    <td>{{ user.created }}</td>
  </tr>
  {% endfor %}
</table>
